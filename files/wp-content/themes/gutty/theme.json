{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"name": "Black and white",
					"slug": "duotone-1",
					"colors": ["#000000", "#ffffff"]
				}
			],
			"gradients": [
				{
					"name": "Vertical hard black to white",
					"slug": "gradient-1",
					"gradient": "linear-gradient(to bottom, #000000 50%, #FFFFFF 50%)"
				},
				{
					"name": "Vertical hard blue to white",
					"slug": "gradient-2",
					"gradient": "linear-gradient(to bottom, #001ea1 50%, #FFFFFF 50%)"
				}
			],
			"palette": [
				{
					"name": "Main",
					"slug": "main",
					"color": "#14111f"
				},
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#001ea1"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#636381"
				},
				{
					"name": "Tertiary",
					"slug": "tertiary",
					"color": "#f2f2ff"
				},
				{
					"name": "Primary Accent",
					"slug": "primary-accent",
					"color": "#CFD6FF"
				},
				{
					"color": "#4B5563",
					"name": "Gray / Primary",
					"slug": "gray-1"
				},
				{
					"color": "#E6E7EB",
					"name": "Gray / Secondary",
					"slug": "gray-2"
				},
				{
					"color": "#F9FAFB",
					"name": "Gray / gray-3",
					"slug": "gray-3"
				},
				{
					"name": "Base",
					"slug": "base",
					"color": "#fff"
				}
			],
			"link": true
		},
		"custom": {
			"fontWeight": {
				"thin": 100,
				"extra-light": 200,
				"light": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"extra-bold": 800,
				"black": 900
			},
			"lineHeight": {
				"none": 1,
				"tight": 1.1,
				"snug": 1.2,
				"normal": 1.5,
				"relaxed": 1.625,
				"loose": 2,
				"body": "calc(1px + 2ex + 7px)"
			}
		},
		"layout": {
			"contentSize": "856px",
			"wideSize": "1216px"
		},
		"position": {
			"sticky": true
		},
		"shadow": {
			"defaultPresets": true,
			"presets": [
				{
					"name": "Natural",
					"slug": "natural",
					"shadow": "6px 6px 9px rgba(0, 0, 0, 0.2)"
				},
				{
					"name": "Deep",
					"slug": "deep",
					"shadow": "12px 12px 50px rgba(0, 0, 0, 0.4)"
				},
				{
					"name": "Sharp",
					"slug": "sharp",
					"shadow": "6px 6px 0px rgba(0, 0, 0, 0.2)"
				},
				{
					"name": "Outlined",
					"slug": "outlined",
					"shadow": "6px 6px 0px -3px rgba(255, 255, 255, 1), 6px 6px rgba(0, 0, 0, 1)"
				},
				{
					"name": "Crisp",
					"slug": "crisp",
					"shadow": "6px 6px 0px rgba(0, 0, 0, 1)"
				},
				{
					"name": "Default",
					"slug": "default",
					"shadow": "0 .5rem 1rem rgba(0,0,0, .15)"
				},
				{
					"name": "Shadow SM",
					"slug": "shadow-sm",
					"shadow": "0 .125rem .25rem rgba(0,0,0, .075)"
				},
				{
					"name": "Shadow LG",
					"slug": "shadow-lg",
					"shadow": "0 1rem 3rem rgba(0,0,0, .175)"
				},
				{
					"name": "Inset",
					"slug": "shadow-inset",
					"shadow": "inset 0 1px 2px rgba(0,0,0, .075)"
				}
			]
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
				  "size": "clamp(.25rem, 1.5vw, .5rem)",
				  "slug": "10",
				  "name": "XS"
				},
				{
				  "size": "clamp(.5rem, 2.5vw, 1rem)",
				  "slug": "20",
				  "name": "S"
				},
				{
				  "size": "clamp(1rem, 4vw, 2rem)",
				  "slug": "30",
				  "name": "M"
				},
				{
				  "size": "clamp(1.5rem, 5vw, 3rem)",
				  "slug": "40",
				  "name": "L"
				},
				{
				  "size": "clamp(2rem, 7vw, 5rem)",
				  "slug": "50",
				  "name": "XL"
				},
				{
				  "size": "clamp(2.5rem, 9vw, 7rem)",
				  "slug": "60",
				  "name": "XXL"
				},
				{
				  "size": "clamp(3rem, 12vw, 12rem)",
				  "slug": "70",
				  "name": "XXXL"
				}
			],
			"units": ["px",	"em", "rem", "vh", "vw", "%"]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": true,
			"writingMode": true,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, avenir next, avenir, segoe ui, helvetica neue, helvetica, Cantarell, Ubuntu, roboto, noto, arial, sans-serif",
					"name": "System Sans-serif",
					"slug": "body"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.8rem",
						"max": "0.9rem"
					},
					"size": "0.85rem",
					"slug": "extra-small",
					"name": "Extra Small"
				},
				{
					"fluid": {
						"min": "0.95rem",
						"max": "1.05rem"
					},
					"size": "1rem",
					"slug": "small",
					"name": "Small"
				},
				{
					"fluid": {
						"min": "1.2rem",
						"max": "1.3rem"
					},
					"size": "1.25rem",
					"slug": "medium",
					"name": "Medium"
				},
				{
					"fluid": {
						"min": "1.825rem",
						"max": "1.925rem"
					},
					"size": "1.875rem",
					"slug": "large",
					"name": "Large"
				},
				{
					"fluid": {
						"min": "2.2rem",
						"max": "2.3rem"
					},
					"size": "2.25rem",
					"slug": "x-large",
					"name": "Extra Large"
				},
				{
					"fluid": {
						"min": "3.7rem",
						"max": "3.8rem"
					},
					"size": "3.75rem",
					"slug": "xx-large",
					"name": "2xl"
				},
				{
					"fluid": {
						"min": "4.2rem",
						"max": "4.3rem"
					},
					"size": "4.25rem",
					"slug": "xxx-large",
					"name": "3xl"
				},
				{
					"fluid": {
						"min": "4.7rem",
						"max": "4.8rem"
					},
					"size": "4.75rem",
					"slug": "xxxx-large",
					"name": "4xl"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/avatar": {
				"border": {
					"radius": "100px"
				}
			},
			"core/button": {
				"border": {
					"width": "1px",
					"style": "solid",
					"color": "transparent"
				},
				"typography": {
					"fontWeight": "600"
				},
				"variations": {
					"outline": {
						"spacing": {
							"padding": {
								"bottom": "calc(0.667em + 2px)",
								"left": "calc(1.333em + 2px)",
								"right": "calc(1.333em + 2px)",
								"top": "calc(0.667em + 2px)"
							}
						},
						"border": {
							"width": "1px",
							"color": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "0.7rem"
				}
			},
			"core/calendar": {
				"color": {
					"text": "var(--wp--preset--color--main)"
				},
				"css": ".wp-block-calendar table:where(:not(.has-text-color)) th{background-color:var(--wp--preset--color--primary-accent);color:var(--wp--preset--color--main);border-color:var(--wp--preset--color--primary-accent)} & table:where(:not(.has-text-color)) td{border-color:var(--wp--preset--color--primary-accent)}"
			},
			"core/categories": {
				"spacing": {
					"padding": {
						"left": "0px",
						"right": "0px"
					}
				},
				"css": "& {list-style-type:none;} & li{margin-bottom: 0.5rem;}"
			},
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				"border": {
					"color": "var(--wp--preset--color--primary)",
					"radius": "var(--wp--preset--spacing--10)",
					"width": "1px",
					"style": "solid"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--20)",
						"left": "var(--wp--preset--spacing--20)",
						"right": "var(--wp--preset--spacing--20)",
						"top": "var(--wp--preset--spacing--20)"
					}
				},
				"typography": {
					"lineHeight": "var(--wp--preset--spacing--30)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-author-name": {
				"color": {
					"text": "var(--wp--preset--color--main)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--main)"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "normal",
					"fontWeight": "600"
				}
			},
			"core/comment-content": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--20)",
						"bottom": "var(--wp--preset--spacing--20)"
					}
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-comments-form": {
				"elements": {
					"heading": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--large)"
						}
					}
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments-pagination": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments-pagination-next": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments-pagination-numbers": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments-pagination-previous": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/details": {
				"border": {
					"radius": "6px"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)",
					"background": "var(--wp--preset--color--tertiary)"
				},
				"shadow": "0 1px 2px 0 rgb(0 0 0 / 0.05)"
			},
			"core/footnotes": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/gallery": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/image": {
				"variations": {
					"rounded": {
						"border": {
							"radius": "var(--wp--preset--spacing--20)"
						}
					}
				}
			},
			"core/media-text": {
				"border": {
					"radius": "var(--wp--preset--spacing--20)"
				}
			},
			"core/list": {
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--20)"
					}
				}
			},
			"core/loginout": {
				"css": "& input{border-radius:var(--wp--preset--spacing--10); border: solid 1px var(--wp--preset--color--primary); padding: 1em; font-size: 1em; line-height: 1em; width: 100%} & #wp-submit {background-color: var(--wp--preset--color--primary); border-radius: var(--wp--preset--spacing--10);border-width: 0;color: var(--wp--preset--color--primary-accent);font-family: inherit;font-size: inherit;line-height: inherit;padding: calc(0.667em + 2px) calc(1.333em + 2px);text-decoration: none;cursor: pointer;}"
			},
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontWeight": "500"
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-author-name": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-excerpt": {
				"typography": {
					"lineHeight": "1.6"
				}
			},
			"core/post-featured-image": {
				"border": {
					"radius": "var(--wp--preset--spacing--20)"
				}
			},
			"core/post-terms": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--loose)"
				},
				"css": "white-space:pre"
			},
			"core/post-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/pullquote": {
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--normal)",
							"fontStyle": "normal"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--40)",
						"top": "var(--wp--preset--spacing--40)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "italic",
					"fontWeight": "400",
					"letterSpacing": "0em",
					"lineHeight": "1.5"
				}
			},
			"core/query-title": {
				"css": "& span {font-style: italic;}"
			},
			"core/query-no-results": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/quote": {
				"elements": {
					"cite": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--normal)",
							"fontStyle": "normal"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--20)",
						"left": "var(--wp--preset--spacing--60)",
						"right": "var(--wp--preset--spacing--60)",
						"top": "var(--wp--preset--spacing--20)"
					},
					"margin": {
						"top": "var(--wp--preset--spacing--40)",
						"bottom": "var(--wp--preset--spacing--40)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "italic",
					"lineHeight": "1.3"
				},
				"variations": {
					"plain": {
						"border": {
							"color": "var(--wp--preset--color--main)",
							"radius": "0",
							"style": "solid",
							"width": "0 0 0 3px"
						},
						"color": {
							"background": "var(--wp--preset--color--tertiary)"
						},
						"typography": {
							"fontStyle": "normal",
							"fontSize": "var(--wp--preset--font-size--medium)",
							"lineHeight": "1.5"
						}
					}
				}
			},
			"core/search": {
				"css": "& .wp-block-search__inside-wrapper {border-radius:var(--wp--preset--spacing--10)}; & .wp-block-search__input{border-radius:var(--wp--preset--spacing--10)}",
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
					"button": {
						"border": {
							"radius": "var(--wp--preset--spacing--10)"
						}
					}
				}
			},
			"core/separator": {
				"border": {
					"color": "currentColor",
					"style": "solid",
					"width": "0 0 1px 0"
				},
				"color": {
					"text": "var(--wp--preset--color--main)"
				}
			},
			"core/site-tagline": {
			},
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "1.2rem",
					"fontStyle": "normal"
				}
			}
		},
		"color": {
			"text": "var(--wp--preset--color--main)",
			"background": "var(--wp--preset--color--base)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--primary-accent)"
				},
				"border":{
					"radius": "var(--wp--preset--spacing--10)"
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--primary)",
						"text": "var(--wp--preset--color--base)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--primary)",
						"text": "var(--wp--preset--color--base)"
					},
					"outline": {
						"color": "var(--wp--preset--color--main)",
						"offset": "2px",
						"style": "dotted",
						"width": "1px"
					},
					"border": {
						"color": "var(--wp--preset--color--main)"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--primary)",
						"text": "var(--wp--preset--color--primary-accent)"
					},
					"border": {
						"color": "var(--wp--preset--color--primary)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
